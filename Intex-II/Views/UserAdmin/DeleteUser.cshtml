@model Microsoft.AspNetCore.Identity.IdentityUser

<br />
<br />
<br />
<h1>Are you sure you want to delete @Model.UserName?</h1>
<form method="post">
    <input type="hidden" asp-for="Id" value="@Model.Id" />
    <button type="submit" class="delete-btn">Delete</button>
    <a asp-controller="UserAdmin" asp-action="UserAdmin" class="cancel-btn">Cancel</a>
</form>
